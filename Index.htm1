// ===== Typing animation for notifications =====
function typeNotification(message, imgSrc=null, i=0, alertDiv=null){
  if(!alertDiv){
    const notifications = document.getElementById("notifications");
    alertDiv = document.createElement("div");
    alertDiv.className = "alert";
    notifications.appendChild(alertDiv);
  }

  if(i < message.length){
    alertDiv.innerHTML = message.substring(0,i+1);
    setTimeout(()=> typeNotification(message,imgSrc,i+1,alertDiv), 50);
  } else if(imgSrc){
    const img = document.createElement("img");
    img.src = imgSrc;
    alertDiv.appendChild(img);
  }
}

// ===== Email input & first auto alert =====
